CREATE DATABASE CodeStorage;

USE CodeStorage;

CREATE TABLE FrontendCode (
    Id INT PRIMARY KEY IDENTITY(1,1),
    Code NVARCHAR(MAX) NOT NULL,
    CreatedAt DATETIME DEFAULT GETDATE()
);

CREATE TABLE backendCode (
    Id INT PRIMARY KEY IDENTITY(1,1),
    Code NVARCHAR(MAX) NOT NULL,
    CreatedAt DATETIME DEFAULT GETDATE()
);
